<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!-- Gradient background -->
    <gradient
        android:startColor="#5580C6"
        android:centerColor="#6690D5"
        android:endColor="#5580C6"
        android:angle="45" />

    <!-- Stroke -->
    <stroke
        android:width="1dp"
        android:color="#FFFFFF" /> <!-- White color stroke -->

    <!-- Corner radius -->
    <corners android:radius="15dp" /> <!-- Adjust the radius as needed -->

</shape>
