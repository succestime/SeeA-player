        <item name="bottomActionModeBackgroundColor">#064566</item>

    </style>


    <style name="statusBarPlaylistTheme" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <item name="themeColor">@color/cool_green</item>
        <item name="colorPrimary">@color/cool_blue</item>
        <item name="colorPrimaryVariant">?attr/themeColor</item>
        <item name="colorOnPrimary">@color/black</item>
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/white</item>
        <item name="android:textColor">@color/white</item>
        <item name="titleTextColor">@color/white</item>
        <item name="android:statusBarColor">@color/transparent</item>
        <item name="android:windowLightStatusBar">false</item> <!-- Light status bar icons -->
        <item name="bottomSheetDialogTheme">@style/AppBottomSheetDiaolgTheme</item>
        <item name="android:windowBackground">#032C42</item>
        <item name="bottomSheetBackgroundColor">#064566</item>
        <item name="bottomActionModeBackgroundColor">#064566</item>
        <item name="alertDialogTheme">@style/CustomAlertDialogTheme</item> <!-- Apply the custom alert dialog theme -->
<item name="android:windowTranslucentStatus">true</item>

    </style>
    <style name="CustomAlertDialogTheme" parent="ThemeOverlay.MaterialComponents.Dialog.Alert">
        <item name="android:background">#064566</item>
        <item name="android:textColor">@color/white</item> <!-- Optional: Set text color for better visibility -->
    </style>







    <style name="TransparentActivityTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:statusBarColor">@color/white</item>
    </style>
    <style name="Theme.Transparent" parent="Theme.AppCompat.DayNight.NoActionBar">
        <item name="android:textColor">@color/white</item>
        <item name="android:statusBarColor" >#0F4D6C</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowLightStatusBar">false</item>


    </style>

    <style name="AppTheme.DarkActionMode" parent="Widget.AppCompat.ActionMode">
        <item name="background">@drawable/background_actionbar</item>
    </style>

    <style name="AppTheme.DarkActionMode.theme" parent="Widget.AppCompat.ActionMode">
        <item name="background">@drawable/background_actionbar_light_three</item>
    </style>

    <style name="AppTheme.LightActionModeBars" parent="Widget.AppCompat.ActionBar">
        <item name="background">@drawable/background_actionbar</item>
    </style>

    <style name="AppTheme.DarkActionBars.themes" parent="Widget.AppCompat.ActionBar">
        <item name="background">@drawable/background_actionbar_light_three</item>
    </style>

    <style name="CustomPopupMenu" parent="Widget.AppCompat.PopupMenu">
        <item name="android:popupBackground">@drawable/popup_menu_background</item>
    </style>



    <style name="Widget.AppCompat.ActionMode.Title" parent="TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textColor">@color/white</item> <!-- Customize title text color -->
    </style>
    <style name="AppBottomSheetDiaolgTheme" parent="Theme.Design.BottomSheetDialog">
        <item name="bottomSheetStyle">@style/AppModelStyle</item>

    </style>

    <style name="AppModelStyle" parent="Widget.Design.BottomSheet.Modal">
        <item name="android:background">@drawable/rounded_bottom_sheet_night</item>

    </style>


    <style name="TransparentTheme" parent="Theme.AppCompat.DayNight">
        <item name="android:statusBarColor">#0F4D6C</item>
        <item name="actionModeStyle">@style/AppTheme.DarkActionMode</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@null</item>
    </style>


